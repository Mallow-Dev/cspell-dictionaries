{
  "name": "multi-agent-dictionary",
  "version": "1.0.0",
  "description": "A multi-agent system for managing a dictionary.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "prepare": "husky"
  },
  "bin": {
    "mallow-librarian": "./agents/librarian/cli.js",
    "mallow-curator": "./agents/curator/cli.js",
    "mallow-gatekeeper": "./agents/gatekeeper/cli.js",
    "mallow-publisher": "./agents/publisher/cli.js",
    "mallow-auditor": "./agents/auditor/cli.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6"
  },
  "devDependencies": {
    "cspell": "^8.6.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,md,json,css,scss,yml,yaml}": [
      "npx cspell --no-color"
    ]
  }
}
